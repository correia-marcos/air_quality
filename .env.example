# .env.example
# Instructions: Copy this file to .env and fill in the values below.

# --- Infrastructure Config (Safe defaults provided) ---
# You can leave these as is for replication.
RSTUDIO_USER=rstudio
RSTUDIO_PASSWORD=replication
IN_DOCKER=true

# --- Data Access Credentials ---
# REQUIRED: If you need to re-download raw data, you must provide your own credentials.
# If you are analyzing the provided static data, you can leave these blank.
EARTHDATA_USER=your_earthdata_username
EARTHDATA_PASS=your_earthdata_password
STADIA_MAPS_API_KEY=your_api_key
STADIA_MAPS_ACCOUNT=your_email
